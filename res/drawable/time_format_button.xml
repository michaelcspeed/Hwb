<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- disabled -->
    <item android:drawable="@drawable/farrow" android:state_enabled="false"/>
    <!-- pressed/down -->
    <item android:drawable="@drawable/farrowdown" android:state_pressed="true"/>
    <!-- focused -->
    <item android:drawable="@drawable/farrowfocused" android:state_enabled="true" android:state_focused="true"/>
    <!-- normal -->
    <item android:drawable="@drawable/farrow" android:state_enabled="true"/>
</selector>